<%= form_for([@case_log, @image], html: {class: "form-model", role: "form"}) do |f| %>
  <div class="field">
    <%= f.label :title, class: :required %><br>
    <%= f.text_field :title %>
  </div>
  <div class="field">
    <%= f.label :url, class: :required %><br>
    <%= f.text_field :url %>
  </div>
  <div class="field">
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </div>
  <div class="actions">
    <div class="form-group">
    <%= f.submit "Submit", class:"btn btn-block btn-info", style:"width: 80px; display:inline;" %> 
    <%= link_to 'Cancel', images_path, class:"btn btn-block btn-danger", style:"width: 80px; display: inline;" %>
  </div>
  </div>
<% end %>
