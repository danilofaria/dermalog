<h1>Share case log</h1>

<strong style="margin-left: 20px; margin-bottom: 10px;">Title:</strong>
<%= @case_log.title %>
<br>
<%= render 'form' %>

<% if @share.errors.any? %>
<div class="error_messages">
  <h2><%= pluralize(@share.errors.count, "error") %> prohibited this share from being saved:</h2>

  <ul>
  <% @share.errors.full_messages.each do |message| %>
